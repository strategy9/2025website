        init() {
            // Only show if not dismissed in this session
            if (!sessionStorage.getItem('blackFridayDismissed')) {
                setTimeout(() => { this.showModal = true; }, 2000);
            }
        },
        closeModal() {
            this.showModal = false;
            sessionStorage.setItem('blackFridayDismissed', 'true');
        }
    }" 
    x-show="showModal"
    x-cloak
    class="fixed inset-0 z-50 overflow-y-auto"
    style="display: none;">
        
        <!-- Backdrop -->
        <div class="fixed inset-0 bg-black bg-opacity-75 transition-opacity" @@click="closeModal()"></div>

